{
  "name": "yoink",
  "version": "0.0.0-alpha",
  "description": "Smart contracts for minting your own NFT tokens",
  "main": "index.js",
  "scripts": {
    "clean": "npx hardhat clean",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "coverage": "yarn typechain && hardhat coverage --solcoverjs ./.solcover.ts --temp artifacts --testfiles \"./test/**/*.ts\"",
    "deploy-local": "npx hardhat run scripts/deploy.ts --network localhost",
    "deploy-rinkeby": "npx hardhat run scripts/deploy.ts --network rinkeby",
    "lint": "yarn lint:sol && yarn lint:ts && yarn prettier:check",
    "lint:sol": "./node_modules/.bin/solhint -f table contracts/**/*.sol",
    "lint:ts": "eslint --config ./.eslintrc.json --ignore-path ./.eslintignore --ext .js,.ts .",
    "prettier": "prettier --config ./.prettierrc.yaml --write \"**/*.{js,json,md,sol,ts}\"",
    "prettier:check": "prettier --check --config ./.prettierrc.yaml \"**/*.{js,json,md,sol,ts}\"",
    "start": "npx hardhat run scripts/run.ts",
    "server": "npx hardhat node",
    "test": "nodemon -x 'npx hardhat test' -w contracts -w test -e ts,sol",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nwthomas/yoink.git"
  },
  "keywords": [
    "Solidity",
    "JavaScript",
    "TypeScript",
    "NFT",
    "Non-Fungible Tokens",
    "Ethereum",
    "EVM",
    "social"
  ],
  "author": "Nathan Thomas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nwthomas/yoink/issues"
  },
  "homepage": "https://github.com/nwthomas/yoink#readme",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@openzeppelin/contracts": "^4.5.0",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eth-provider": "^0.10.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.9.1",
    "hardhat-gas-reporter": "^1.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "shelljs": "^0.8.5",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.16",
    "ts-node": "^10.1.0",
    "typechain": "^5.1.2",
    "typescript": "^4.5.2"
  }
}
